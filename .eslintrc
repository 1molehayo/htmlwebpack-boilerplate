{
  "extends": ["prettier"],
  "parser": "babel-eslint",
  "rules": {
    "comma-dangle": 0,
    "linebreak-style": 0,
    "global-require": "off",
    "prefer-spread": "off",
    "arrow-parens": "off",
    "no-console": 1,
    "no-restricted-syntax": "off",
    "no-plusplus": 0,
    "array-callback-return": 2,
    "block-scoped-var": 2,
    "curly": 2,
    "eqeqeq": 2,
    "no-alert": 1,
    "no-else-return": 1,
    "no-empty-function": 1,
    "class-methods-use-this": "off",
    "no-nested-ternary": "off",
    "no-underscore-dangle": "off",
    "consistent-return": "off",
    "no-unused-expressions": [
      2,
      {
        "allowTernary": true
      }
    ]
  }
}
